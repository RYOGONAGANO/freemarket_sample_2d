
.userRegist-complete-header
  .userRegist-complete-header-headers
    .userRegist-complete-header-headers__mercarilogo
      = image_tag 'fmarket_logo_red.svg', id:'assets', class:'image', size:'185x45'
    .userRegist-complete-header-headers__status
      = render 'shared/new_user_progress'
.userRegist-complete-contents
  .userRegist-complete-uppercontent
    %h2会員登録完了
  .userRegist-complete-rowercontent
    .userRegist-complete-rowercontent__text
      %pありがとうございます。
      %p会員登録が完了しました。
    = link_to "Fmarketをはじめる", root_path, class: "userRegist-complete-rowercontent__box"
  .userRegist-complete-advertisement
    = image_tag 'logo_gray.svg', size: "700x250"
=render 'shared/copywrite-bottom'